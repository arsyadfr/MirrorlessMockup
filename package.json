{
  "name": "MIRRORLESS",
  "version": "1.0.0",
  "description": "Capture Without Limits.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --mac",
    "dist": "electron-builder --mac"
  },
  "author": "Arsyad Faiz",
  "license": "1.0",
  "devDependencies": {
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.arsyadfaiz.mirrorless",
    "productName": "MIRRORLESS",
     "publish": [
    {
      "provider": "github",
      "owner": "arsyadfr",
      "repo": "MirrorlessMockup",
      "publishAutoUpdate": true
    }
  ],
    "mac": {
      "target": "dmg"
    },
    "directories": {
      "output": "dist"
    }
  }
}
